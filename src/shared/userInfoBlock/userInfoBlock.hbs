<link rel="stylesheet" href="../../shared/userInfoBlock/userInfoBlock.scss"/><div class="userInfoBlockContainer">    <div class="userInfoBlockWrapper">        {{> avatar/avatar src="../../assets/img/avatar.png" alt='avatar' class="userAvatar" }}        <h4>{{userName}}</h4>        <div class="userEditedBlock">            {{> infoItemInput/infoItemInput title="Почта" editInfo="pochta@yandex.ru" class="infoItemInput" }}            {{> infoItemInput/infoItemInput title="Логин" editInfo="ivanivanov" class="infoItemInput" }}            {{> infoItemInput/infoItemInput title="Имя" editInfo="Иван" class="infoItemInput" }}            {{> infoItemInput/infoItemInput title="Фамилия" editInfo="Иванов" class="infoItemInput" }}            {{> infoItemInput/infoItemInput title="Имя в чате" editInfo="Иван" class="infoItemInput" }}            {{> infoItemInput/infoItemInput title="Телефон" editInfo="+7 (909) 967 30 30" class="infoItemInput" }}        </div>        <div class="userSettingsBlock">            {{> infoItemInput/infoItemInput title="Изменить данные" editInfo="" class="infoChangedInput"}}            {{> infoItemInput/infoItemInput title="Изменить пароль" editInfo="" class="infoChangedInput"}}            <div class="logout">                <span>Выйти</span>                <a>{{editInfo}}</a>            </div>        </div>    </div></div>