<link rel="stylesheet" href="../../shared/chatsItem/chatsItem.scss" /><div class="chatContainer">  <div class="chatItem">    {{> avatar/avatar src=src alt='avatar' class="chatAvatar" }}    <div class="chatWrapper">      <div class="userName">{{userName}}</div>      <div class="chatMessage">        {{#if senderName}} <strong>{{senderName}}: </strong>{{/if}}        {{textMessage}}      </div>    </div>    <div class="chatInfo">      <div class="sendingTime">{{sendingTime}}</div>      <div class="counterMsg">{{number}}</div>    </div>  </div></div>